{
    "buffer_size": 1500,
    "queue_size": 10240,
    "services":[
        {
            "type": "listen",
            "tag": "client_listen",
            "listen_addr": "0.0.0.0:7000",
            "timeout": 120,
            "worker_count": 1,
            "replace_old_mapping": true,
            "send_keepalive": true,
            "detour": [
                "client_forward"
            ]
        },
        {
            "type": "forward",
            "tag": "client_forward",
            "forwarders": [
                "server1.example.com:9000",
                "server2.example.com:9000"
            ],
            "reconnect_interval": 5,
            "connection_check_time": 30,
            "send_keepalive": true,
            "detour": [
                "client_listen"
            ]
        }
    ]
}