{
    "buffer_size": 1500,
    "queue_size": 10240,
    "services":[
        {
            "type": "listen",
            "tag": "server_listen",
            "listen_addr": "0.0.0.0:9000",
            "timeout": 120,
            "worker_count": 1,
            "replace_old_mapping": true,
            "detour": [
                "server_forward"
            ]
        },
        {
            "type": "forward",
            "tag": "server_forward",
            "forwarders": [
                "127.0.0.1:3000"
            ],
            "reconnect_interval": 5,
            "connection_check_time": 30,
            "send_keepalive": true,
            "detour": [
                "server_listen"
            ]
        }
    ]
}