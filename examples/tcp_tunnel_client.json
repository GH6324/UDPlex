{
    "buffer_size": 1600,
    "queue_size": 10240,
    "worker_count": 4,
    "logging": {
        "level": "debug",
        "format": "console",
        "output_path": "stdout",
        "caller": true
    },
    "services":[
        {
            "type": "listen",
            "tag": "client_listen",
            "listen_addr": "0.0.0.0:5202",
            "timeout": 120,
            "replace_old_mapping": true,
            "detour": [
                "client_forward"
            ]
        },
        {
            "type": "tcp_tunnel_forward",
            "tag": "client_forward",
            "forwarders": [
                "127.0.0.1:5203:2" // ip:port:conn_count
            ],
            "connection_check_time": 10,
            "detour": [
                "client_listen"
            ],
            "auth": {
                "secret": "your-secret-key-here",
                "enabled": true,
                "enable_encryption": true,
                "heartbeat_interval": 30
            }
        }
    ]
}